<?xml version='1.0' encoding='UTF-8'?>
<Configuration status='DEBUG'>
  <Appenders>
    <Console name='LogToConsole' target='SYSTEM_OUT'>
      <PatternLayout pattern='%d{HH:mm:ss.SSS} %-5level - %msg%n' />
    </Console>
  </Appenders>
  <Loggers>
    <Root level='debug'>
      <AppenderRef ref='LogToConsole'/>
    </Root>
  </Loggers>
</Configuration>
